event AddDeposit(
    address indexed account,
    address indexed token,
    uint256 season,
    uint256 amount,
    uint256 bdv
);

event RemoveDeposits(
    address indexed account,
    address indexed token,
    uint32[] seasons,
    uint256[] amounts,
    uint256 amount
);

event RemoveDeposit(
    address indexed account,
    address indexed token,
    uint32 season,
    uint256 amount
);

event AddWithdrawal(
    address indexed account,
    address indexed token,
    uint32 season,
    uint256 amount
);

event RemoveWithdrawals(
    address indexed account,
    address indexed token,
    uint32[] seasons,
    uint256 amount
);

event RemoveWithdrawal(
    address indexed account,
    address indexed token,
    uint32 season,
    uint256 amount
);
